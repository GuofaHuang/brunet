<!-- It is strongly suggested that you change the brunet_namespace so that you do not have collision! -->
<IPRouterConfig>
  <ipop_namespace>undefined</ipop_namespace>
  <brunet_namespace>Local_Testing</brunet_namespace>
  <EdgeListeners>
    <EdgeListener type="udp"/>
    <EdgeListener type="tcp"/>
  </EdgeListeners>
  <RpcDht>
    <Enabled>true</Enabled>
    <Port>64221</Port>
  </RpcDht>
  <XmlRpcManager>
    <Enabled>true</Enabled>
    <Port>10000</Port>
  </XmlRpcManager>
</IPRouterConfig>
