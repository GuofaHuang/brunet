  <brunet_namespace>Local_Testing</brunet_namespace>
  <device>tap0</device>
  <RemoteTAs>
    <transport>brunet.udp://127.0.0.1:45391</transport>
    <transport>brunet.tcp://127.0.0.1:42391</transport>
  </RemoteTAs>
  <EdgeListeners>
    <EdgeListener type="udp">
      <port>45391</port>
