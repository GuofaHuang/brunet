<!-- This can be placed into the Dht for DhtDHCP, dht_dhcp_namespace is a 
     condensed version -->
<DHCPServerConfig>
  <brunet_namespace>condor_wow</brunet_namespace>
  <ipop_namespace>
    <IPOPNamespace>
      <value>Condor</value>
      <netmask>255.192.0.0</netmask>
      <pool>
        <lower>10.129.0.0</lower>
        <upper>10.131.255.255</upper>
      </pool>
      <reserved>
        <value>
          <DHCPReservedIP>
            <ip>0.0.0.2</ip>
            <mask>0.0.0.255</mask>
          </DHCPReservedIP>
        </value>
      </reserved>
      <leasetime>604800</leasetime>
      <LogSize>20480</LogSize>
    </IPOPNamespace>
    <IPOPNamespace>
      <value>Test</value>
      <netmask>255.192.0.0</netmask>
      <pool>
        <lower>10.191.255.0</lower>
        <upper>10.191.255.255</upper>
      </pool>
      <leasetime>86400</leasetime>
      <LogSize>5120</LogSize>
    </IPOPNamespace>
  </ipop_namespace>
</DHCPServerConfig>
